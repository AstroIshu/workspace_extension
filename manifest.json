{
  "manifest_version": 2,
  "name": "AstroIshu WorkFlow Manager",
  "version": "1.0",
  "description": "Manage multiple workspaces with streamlined workflow.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "ishu1616.jpg",
      "48": "ishu4848.jpg",
      "128": "ishu128128.jpg"
    }
  },
  "icons": {
    "16": "ishu1616.jpg",
    "48": "ishu4848.jpg",
    "128": "ishu128128.jpg"
  }
}
